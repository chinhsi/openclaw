{
  "build": {
    "dockerfile": "./Dockerfile.zeabur"
  },
  "env": {
    "PORT": {
      "default": "3000"
    },
    "NODE_ENV": {
      "default": "production"
    },
    "OPENCLAW_CONFIG_PATH": {
      "default": "/app/openclaw.json"
    }
  },
  "ports": [
    {
      "port": 3000,
      "protocol": "HTTP"
    }
  ],
  "deploy": {
    "auto_deploy": true,
    "branch": "main"
  }
}